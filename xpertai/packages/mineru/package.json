    "@langchain/langgraph": "0.4.7",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@xpert-ai/plugin-sdk": "^3.6.2",
    "@metad/contracts": "^3.6.2",
    "axios": "1.12.2",
    "nestjs-i18n": "10.5.1",
    "chalk": "4.1.2",
